<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <_PropertySheetDisplayName>BuildSample</_PropertySheetDisplayName>
    <OutDir>..\..\bin\$(Platform)\</OutDir>
    <IntDir>..\..\obj\$(ProjectName)\$(Configuration)$(Platform)\</IntDir>
    <TargetName>$(ProjectName)$(Configuration)</TargetName>
    <LibraryPath>..\..\lib\$(Platform)\;$(PCSDK_DIR)\lib\$(Platform);$(LibraryPath)</LibraryPath>
    <IncludePath>..\..\include;$(PCSDK_DIR)\include;$(IncludePath)</IncludePath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile />
    <Link />
    <ClCompile>
      <AdditionalIncludeDirectories>
      </AdditionalIncludeDirectories>
    </ClCompile>
    <Link />
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>